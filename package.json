{
  "name": "hjson",
  "displayName": "Hjson",
  "description": "Hjson language syntax support",
  "version": "0.3.2",
  "publisher": "laktak",
  "repository": "https://github.com/hjson/vscode-hjson",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hjson",
        "aliases": [
          "Hjson",
          "hjson"
        ],
        "extensions": [
          ".hjson"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hjson",
        "scopeName": "source.hjson",
        "path": "./syntaxes/Hjson.tmLanguage"
      }
    ]
  }
}
